{
  "name": "agave-cli",
  "description": "create votes in the Agave DAO that change parameters of the Agave Protocol",
  "version": "0.0.0",
  "author": "honeyismoney @HoneyIsMoney",
  "bin": {
    "agave": "./bin/run"
  },
  "bugs": "https://github.com/HoneyIsMoney/agave-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@types/inquirer": "^7.3.3",
    "cli-ux": "^5.6.3",
    "dao-evm-helpers": "^1.0.0",
    "eth-provider": "^0.9.4",
    "ethers": "^5.4.6",
    "inquirer": "^8.1.2",
    "tslib": "^1",
    "web3-eth-abi": "^1.5.2",
    "web3-utils": "^1.5.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/HoneyIsMoney/agave-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "agave",
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "security:pause": {
        "description": "Pause the protocol using the Security Board"
      },
      "security:unpause": {
        "description": "Unpause the protocol using the Security Board"
      }
    }
  },
  "repository": "HoneyIsMoney/agave-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
